<%= form_for([taggroup,tag],html: { html: "form-horizontal" }, remote: true) do |f| %>
  <p>
    <b>Label</b>
    <%= f.text_field :label %>
  </p>
  <p>
    <b>Type</b>
    <%= f.select :tag_type, tag.type_map, { :include_blank => false}, {:class => "chosen-select", "data-placeholder" => "选择[标签]类型"} %>
  </p>
  <p>
    <%= f.submit "submit", class: "btn btn-primary" %>
  </p>
<% end %>

