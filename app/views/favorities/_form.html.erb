<%= form_for(favority,html: { html: "form-horizontal" }, remote: true) do |f| %>
  <p>
    <b>name</b>
    <%= f.text_field :label %>
  </p>
  <p>
    <b>desc</b>
    <%= f.text_area :desc,rows: 10 %>
  </p>
  <p>
    <b>type</b>
    <%= f.text_field :klass %>
  </p>
  <p>
    <%= f.submit "submit", class: "btn btn-primary" %>
  </p>
<% end %>
